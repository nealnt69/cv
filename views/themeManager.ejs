<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta http-equiv="Content-Language" content="en" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Dashboard</title>
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no, shrink-to-fit=no"
    />
    <meta name="msapplication-tap-highlight" content="no" />
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <script
      src="https://kit.fontawesome.com/eb123fc88b.js"
      crossorigin="anonymous"
    ></script>
    <link href="/stylesheets/theme.css" rel="stylesheet" />
    <link rel="stylesheet" href="/stylesheets/themeManager.css" />
  </head>
  <body>
    <div
      class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header"
    >
      <div class="app-header header-shadow">
        <div class="app-header__logo">
          <div class="logo-src" style="height: 42px">
            <img
              src="https://beta.ketnoiviec.net/static/media/logo-main.2eda1ca0.svg"
              alt=""
              style="width: 42px"
            />
          </div>
          <div class="header__pane ml-auto">
            <div>
              <button
                type="button"
                class="hamburger close-sidebar-btn hamburger--elastic"
                data-class="closed-sidebar"
              >
                <span class="hamburger-box">
                  <span class="hamburger-inner"></span>
                </span>
              </button>
            </div>
          </div>
        </div>
        <div class="app-header__mobile-menu">
          <div>
            <button
              type="button"
              class="hamburger hamburger--elastic mobile-toggle-nav"
            >
              <span class="hamburger-box">
                <span class="hamburger-inner"></span>
              </span>
            </button>
          </div>
        </div>
        <div class="app-header__menu">
          <span>
            <button
              type="button"
              class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav"
            >
              <span class="btn-icon-wrapper">
                <i class="fa fa-ellipsis-v fa-w-6"></i>
              </span>
            </button>
          </span>
        </div>
        <div class="app-header__content">
          <div class="app-header-left">
            <div class="search-wrapper">
              <div class="input-holder">
                <input
                  type="text"
                  class="search-input"
                  placeholder="Type to search"
                />
                <button class="search-icon"><span></span></button>
              </div>
              <button class="close"></button>
            </div>
          </div>
          <div class="app-header-right"></div>
        </div>
      </div>

      <div class="app-main">
        <div class="app-sidebar sidebar-shadow">
          <div class="app-header__logo" style="width: 42px">
            <div class="logo-src">
              <img
                src="https://ketnoiviec.net/static/media/main.c5e8fa5c.svg"
                style="width: 42px"
                alt=""
              />
            </div>
            <div class="header__pane ml-auto">
              <div>
                <button
                  type="button"
                  class="hamburger close-sidebar-btn hamburger--elastic"
                  data-class="closed-sidebar"
                >
                  <span class="hamburger-box">
                    <span class="hamburger-inner"></span>
                  </span>
                </button>
              </div>
            </div>
          </div>
          <div class="app-header__mobile-menu">
            <div>
              <button
                type="button"
                class="hamburger hamburger--elastic mobile-toggle-nav"
              >
                <span class="hamburger-box">
                  <span class="hamburger-inner"></span>
                </span>
              </button>
            </div>
          </div>
          <div class="app-header__menu">
            <span>
              <button
                type="button"
                class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav"
              >
                <span class="btn-icon-wrapper">
                  <i class="fa fa-ellipsis-v fa-w-6"></i>
                </span>
              </button>
            </span>
          </div>
          <div class="scrollbar-sidebar">
            <div class="app-sidebar__inner">
              <ul class="vertical-nav-menu">
                <li class="app-sidebar__heading">Quản lý cv</li>
                <li>
                  <a href="/dashboard/tao-mau">
                    <i class="fa fa-plus" aria-hidden="true"></i>
                    Tạo mẫu cv
                  </a>
                </li>
                <li>
                  <a href="/dashboard/them-mau">
                    <i class="fa fa-file-pdf-o" aria-hidden="true"></i>
                    Thêm mẫu cv
                  </a>
                </li>
                <li>
                  <a href="/dashboard/quan-ly-mau">
                    <i class="fa fa-book" aria-hidden="true"></i>
                    Quản lý mẫu cv
                  </a>
                </li>
                <li class="app-sidebar__heading">Thống kê</li>
                <li>
                  <a href="#">
                    <i class="fa fa-file-pdf-o" aria-hidden="true"></i>
                    Thống kê lượng truy cập
                  </a>
                </li>
                <li>
                  <a href="#">
                    <i class="fa fa-book" aria-hidden="true"></i>
                    Thông kê cv được tạo
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </div>
        <div class="app-main__outer">
          <div class="app-main__inner">
            <div class="app-page-title">
              <div class="page-title-wrapper">
                <div class="page-title-heading">
                  <div class="page-title-icon">
                    <i class="fa fa-list" aria-hidden="true"></i>
                    </i>
                  </div>
                  <div>Danh sách CV</div>
                </div>
              </div>
            </div>
            <div class="row">
              <% themes.forEach(function(theme){ %>
              <div class="col-xs-6 col-sm-6 col-md-4 col-lg-2 cv-manager-item">
                  <a href="/dashboard/sua-mau/<%= theme._id %>">
                <img class="image-cv-manager" src="<%= theme.image %>" alt="" />
                    </a>
                <p class="card-footer-item"><%= theme.title %></p>
              </div>
              <% }); %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>
  <script type="text/javascript" src="/javascripts/theme.js"></script>
</html>
